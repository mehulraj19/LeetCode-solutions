class Solution {
public:
    string toLowerCase(string str) {
        string k;
        for(int i=0; i<str.length(); i++) {
            int l = str[i];
            if(l>=97){
                k+= str[i];
            }else if(l>= 65){
                k += str[i] + 32;
            }else{
                k+= str[i];
            }
        }
        return k;
    }
};
